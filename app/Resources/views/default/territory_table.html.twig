            <tr>
                {% for empire in empires %}
                    <th class="empire{{empire.id}}">{{ empire.empire }}</th>
                {% endfor %}
            </tr>

            {% for row in rows %}
            <tr>
                {% for cell in row %}
                    <td class="empire{{cell.color}}">{{cell.name}}</td>
                {% endfor %}
            </tr>
            {% endfor %}